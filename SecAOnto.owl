    </owl:Class>
    


    <!-- http://www.NewOnto1.org/SecAOnto_V1#Asset -->

    <owl:Class rdf:about="http://www.NewOnto1.org/SecAOnto_V1#Asset">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.NewOnto1.org/SecAOnto_V1#hasDefect"/>
                <owl:someValuesFrom rdf:resource="http://www.NewOnto1.org/SecAOnto_V1#Defect"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
